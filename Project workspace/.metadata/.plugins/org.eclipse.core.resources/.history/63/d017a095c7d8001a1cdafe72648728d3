package Snake;

import java.awt.Color;
import java.awt.Graphics;

public class Token {

		private int x, y, score;
		private Snake snake;
		
		public Token(Snake s) {
			
			snake = s;
		}
		
		public int getScore() {
			return score;
		}
		
		public void draw(Graphics g) {
			g.setColor(Color.green);
			g.fillRect(x, y, 6, 6);
		}
}
